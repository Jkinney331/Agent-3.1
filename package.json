    "d3": "^7.9.0",
    "date-fns": "^3.6.0",
    "dotenv": "^16.6.1",
    "framer-motion": "^11.0.0",
    "joi": "^17.12.2",
    "jsonwebtoken": "^9.0.0",
    "lightweight-charts": "^4.2.3",
    "lucide-react": "^0.400.0",
    "mcp-client": "^1.13.0",
    "n8n": "^1.102.4",
    "next": "^14.2.0",
    "next-themes": "^0.3.0",
    "node-fetch": "^3.3.2",
    "node-telegram-bot-api": "^0.64.0",
    "node-vault": "^0.10.2",
    "openai": "^5.10.1",
