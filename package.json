    "socket.io-client": "^4.7.0",
    "sonner": "^2.0.6",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "tailwind-merge": "^2.2.0",
    "tailwindcss": "^3.4.0",
    "telegraf": "^4.16.3",
