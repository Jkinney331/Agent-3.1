    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-tooltip": "^1.0.0",
    "@supabase/supabase-js": "^2.52.1",
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/typography": "^0.5.0",
    "@tanstack/react-query": "^5.0.0",
    "@types/crypto-js": "^4.2.0",
    "@types/d3": "^7.4.0",
