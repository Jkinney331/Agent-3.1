{
  "name": "ai-trading-bot-dashboard",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "test:performance": "jest --config jest.performance.config.js",
    "test:comprehensive": "node scripts/execute-comprehensive-tests.js",
    "test:phase3": "npm run test:comprehensive",
    "mcp:crypto": "node lib/mcp/crypto-server.js",
    "mcp:alpha": "node lib/mcp/alpha-vantage-server.js",
    "mcp:free": "node lib/mcp/free-crypto-analytics-server.js",
    "mcp:test": "curl http://localhost:3000/api/crypto?action=trending",
    "mcp:all": "concurrently \"npm run mcp:crypto\" \"npm run mcp:alpha\" \"npm run mcp:free\"",
    "mcp:trading": "node lib/mcp/trading-execution-server.js",
    "trading:start": "concurrently \"npm run dev\" \"npm run mcp:trading\"",
    "security:encrypt": "node scripts/encrypt-credentials.js",
    "security:setup": "node scripts/setup-vault.js",
    "integrated:start": "ts-node scripts/start-integrated-trading-system.ts",
    "integrated:start:test": "ts-node scripts/start-integrated-trading-system.ts --test",
    "integrated:start:no-telegram": "ts-node scripts/start-integrated-trading-system.ts --no-telegram",
    "integrated:test": "ts-node scripts/test-complete-integration.ts",
    "integrated:test:quick": "npm run integrated:test -- --quick",
    "test:alpaca": "node scripts/alpaca-integration-demo.js",
    "test:alpaca:detailed": "node scripts/simple-alpaca-test.js",
    "telegram:setup": "echo 'Setup Telegram bot token in .env.local'",
    "telegram:test": "node scripts/test-telegram-bot.js",
    "ai:test": "echo 'Test AI reasoning engine'",
    "stops:test": "echo 'Test dynamic trailing stops'",
    "db:setup": "node scripts/setup-local-database.js",
    "db:seed": "node scripts/seed-test-data.js",
    "db:migrate": "node scripts/run-migration.js",
    "dev:full": "concurrently \"npm run db:setup\" \"npm run dev\" \"npm run mcp:all\"",
    "test:local": "node scripts/test-local-environment.js",
    "docker:dev": "docker-compose up postgres redis n8n",
    "docker:full": "docker-compose --profile app up",
    "docker:monitoring": "docker-compose --profile monitoring up",
    "ngrok:start": "docker-compose --profile ngrok up ngrok"
  },
  "dependencies": {
    "@alpacahq/alpaca-trade-api": "^3.0.3",
